  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1",
      "^@core/(.*)$": "<rootDir>/core/$1",
      "^@integrations/(.*)$": "<rootDir>/integrations/$1"
    },
    "collectCoverageFrom": [
